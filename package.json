{
  "name": "@uniswap/v2-core",
  "description": "🎛 Core contracts for the UniswapV2 protocol",
  "version": "1.0.1",
  "homepage": "https://uniswap.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/Uniswap/uniswap-v2-core"
  },
  "keywords": [
    "uniswap",
    "ethereum",
    "v2",
    "core",
    "uniswap-v2"
  ],
  "files": [
    "contracts",
    "build"
  ],
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^5.3.0",
    "@openzeppelin/contracts-upgradeable": "^5.3.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "hardhat": "^2.25.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "lint": "yarn prettier ./test/*.ts --check",
    "lint:fix": "yarn prettier ./test/*.ts --write",
    "clean": "rimraf ./build/",
    "precompile": "yarn clean",
    "compile": "npx hardhat compile",
    "pretest": "yarn compile",
    "test": "mocha",
    "prepublishOnly": "yarn test",
    "deploy:ganache": "npx hardhat run scripts/deploy.ts --network ganache",
    "deploy:tenderly": "npx hardhat run scripts/deploy.ts --network tenderly"
  },
  "license": "GPL-3.0-or-later"
}
